{
    "manifest_version": 2,
    "name": "cryptocoin-chrome-extension",
    "description": "CryptoCoin Chrome Extension",
    "version": "0.0.1",
    "author": "mamanogit",
    "content_security_policy" : "script-src 'self' https://ajax.googleapis.com;      object-src 'self'",
    "browser_action": {
      "default_title": "",
      "default_popup": "main.html"
    },
    "storage": {
      "managed_schema": "schema.json"
    },
    "icons": { 
      "16": "assets/img/icon16.png"
    },
    "permissions": [
      "storage",
      "tabs",
      "notifications",
      "http://*/",
      "https://*/",
      "https://ajax.googleapis.com/"
    ]
  }